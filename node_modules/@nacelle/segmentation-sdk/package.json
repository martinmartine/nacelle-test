    "@typescript-eslint/parser": "^2.16.0",
    "axios": "^0.19.1",
    "babel-plugin-import-graphql": "^2.7.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
