{
  "name": "@nacelle/nacelle-nuxt-module",
  "version": "5.5.3",
  "description": "Nacelle module for usage with Nuxt",
  "license": "MIT",
  "contributors": [
    {
      "name": "Nacelle"
    }
  ],
  "main": "lib/module.js",
  "repository": "https://github.com/ShopElf/nacelle-nuxt-module",
  "scripts": {
    "dev": "nuxt example",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ./lib",
    "test:ci": "jest --runInBand --coverage",
    "test": "jest",
    "release": "yarn test && standard-version && git push --follow-tags && npm publish"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@nacelle/client-js-sdk": "^2.4.0",
    "@nacelle/segmentation-sdk": "^0.0.2",
    "axios": "^0.19.2",
    "core-js": "^2.6.10",
    "dotenv": "^8.2.0",
    "fs-extra": "^9.0.0",
    "node-fetch": "^2.6.1",
    "node-sass": "^4.13.0",
    "sass-loader": "^7.3.1",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.11.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/jest": "^26.0.8",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "latest",
    "babel-jest": "^26.2.2",
    "codecov": "latest",
    "eslint": "^7.5.0",
    "eslint-plugin-ejs": "0.0.2",
    "eslint-plugin-vue": "^6.2.2",
    "get-port": "latest",
    "husky": "^2.7.0",
    "jest": "^26.2.2",
    "lint-staged": "^10.2.11",
    "nuxt-edge": "^2.12.2-26419695.8d355956",
    "prettier": "^2.0.5",
    "request": "latest",
    "standard-version": "latest",
    "vue": "^2.6.12",
    "vue-jest": "^3.0.6",
    "vue-router": "^3.1.6",
    "vue-template-compiler": "^2.6.12"
  },
  "lint-staged": {
    "src/**/*.(js|ts)": [
      "prettier --write",
      "npm run lint"
    ],
    "*{json, md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  }
}
